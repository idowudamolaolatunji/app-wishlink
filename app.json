{
  "expo": {
    "name": "Wishers",
    "slug": "app-wishers",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/svgs/logo.svg",
    "scheme": "appwishers",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.idowudamolaolatunji.appwishers",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
      //   "backgroundColor": "#E6F4FE",
      //   "foregroundImage": "./assets/images/android-icon-foreground.png",
      //   "backgroundImage": "./assets/images/android-icon-background.png",
      //   "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.idowudamolaolatunji.appwishers",
      "permissions": [
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/svgs/logo.svg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": null,
          // "imageWidth": 200,
          // "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#111111"
          }
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow Wishers App to use Face ID."
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "74821660-2c77-4928-8044-940537c985f5"
      }
    }
  }
}
